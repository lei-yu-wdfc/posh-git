<?xml version="1.0" encoding="utf-8" ?>
<Ui>
	<MyPaymentsPage>
		<PopupBankName>#edit-bank-name</PopupBankName>
		<PopupBankAccountType>#edit-bank-account-type</PopupBankAccountType>
		<PopupAccountNumber>#edit-account-number-za</PopupAccountNumber>
		<PopupLengthOfTime>#edit-bank-period</PopupLengthOfTime>
		<PopupAddBankAccountButton>#edit-submit</PopupAddBankAccountButton>
		<PopupExeption>#wonga-my-account-edit-bank-form div:nth-child(1) ul li</PopupExeption>
		<AccountNumber>.info.bank-account.bank-account-primary tbody tr:nth-child(4) td b span</AccountNumber>
		<PopupBankAccountException>#edit-account-number-za-wrapper div.form-error div.invalid </PopupBankAccountException>
    <PopupSuccessTitle>h1#success</PopupSuccessTitle>
	</MyPaymentsPage>
	
  <ContactingYouSection>
    <HomePhone>#edit-home-phone-za</HomePhone>
    <MobilePhone>#edit-mobile-phone-za</MobilePhone>
  </ContactingYouSection>

  <MobilePinVerificationSection>
    <Pin>#edit-pin-za</Pin>
	<ResendPin>.resend-pin.mod</ResendPin>
	<ResendPinMessage>#fancybox-content div div p</ResendPinMessage>  
  </MobilePinVerificationSection>
  
  <EmploymentDetailsSection>
    <WorkPhone>#edit-work-phone-za</WorkPhone>
    <NextPaydayDateDay>#edit-next-payday-day</NextPaydayDateDay>
    <NextPaydayDateMonth>#edit-next-payday-month</NextPaydayDateMonth>
    <NextPaydayDateYear>#edit-next-payday-year</NextPaydayDateYear>
  </EmploymentDetailsSection>

  <PersonalDetailsPage>
	<GenderWarning>.//*[@id='details']/div[2]/div/div[3]/div</GenderWarning>
	<DateOfBirthWarning>.//*[@id='edit-date-of-birth-wrapper']/div[3]/div</DateOfBirthWarning>
    <MarriedInCommunityProperty>input[type="radio"][name="married"]</MarriedInCommunityProperty>
  </PersonalDetailsPage>
  
  <PersonalBankAccountDetailsPage>
    <FormId>#wonga-apply-lzero-form</FormId>
  </PersonalBankAccountDetailsPage>
  
  <AccountDetailsPage>
    <FormId>#wonga-apply-lzero-form</FormId>
    <NextButton>#edit-submit</NextButton>
  </AccountDetailsPage>

  <AccountDetailsSection>
    <Fieldset>#account-setup</Fieldset>
	<PasswordErrorForm>#edit-password-wrapper .form-error .invalid</PasswordErrorForm>
    <PasswordConfirmErrorForm>#edit-password-confirm-wrapper .form-error .invalid</PasswordConfirmErrorForm>
  </AccountDetailsSection>

  <DealDonePage>
    <HeaderText>Your application has been accepted, you’re almost there!</HeaderText>
  </DealDonePage>
  
  <AddressDetailsPage>
    <FormId>#wonga-apply-lzero-form</FormId>
    <District>#edit-district-za</District>
    <HouseNumber>#edit-house-number-za</HouseNumber>
    <Town>#edit-town-za</Town>
    <County>#edit-county-za</County>
    <AddressPeriod>#edit-address-period-za</AddressPeriod>
    <Street>#edit-street-za</Street>
    <Postcode>#edit-postcode-za</Postcode>
    <PostcodeErrorForm>#edit-postcode-za-wrapper div.form-error div.invalid</PostcodeErrorForm>
    <CountyErrorForm>#edit-county-za-wrapper div.invalid</CountyErrorForm>
    <TownErrorForm>#edit-town-za-wrapper div.invalid</TownErrorForm>
    <StreetErrorForm>#edit-street-za-wrapper div.invalid</StreetErrorForm>
    <HouseNumberErrorForm>#edit-house-number-za-wrapper div.invalid</HouseNumberErrorForm>
    <AddressPeriodErrorForm>#edit-address-period-za-wrapper div.invalid</AddressPeriodErrorForm>
  </AddressDetailsPage>

  <AcceptedPage>
    <DetailsTable>div#agreementscroll div:nth-child(3) table tbody</DetailsTable>
    <LoanAmount>tr:nth-child(1)>td:nth-child(2)</LoanAmount>
    <PaymentDueDate>tr:nth-child(6)>td:nth-child(2)</PaymentDueDate>
    <TotalToPayOnPaymentDate>tr:nth-child(11)>td:nth-child(2)</TotalToPayOnPaymentDate>
  </AcceptedPage>
  
  <BankAccountSection>
    <Fieldset>#bank</Fieldset>
    <AccountNumber>#edit-account-number-za</AccountNumber>
  </BankAccountSection>

  <MyAccountNavigationSection>
    <!-- Should be extended-->
    <MySummary>div#block-wonga-6 ul.menu>li:nth-child(1)>a</MySummary>
    <MyPersonalDetails>div#block-wonga-6 ul.menu>li:nth-child(2)>a</MyPersonalDetails>
    <MyPaymentsDetails>div#block-wonga-6 ul.menu>li:nth-child(3)>a</MyPaymentsDetails>
  </MyAccountNavigationSection>

  <MyPersonalDetailsPage>
    <Address>table#profile tr#address</Address>
    <HouseNumberAndStreet>table#profile tr#address tbody tr:nth-child(1) td</HouseNumberAndStreet>
    <Town>table#profile tr#address tbody tr:nth-child(2) td</Town>
    <Postcode>table#profile tr#address tbody tr:nth-child(3) td</Postcode>
    <Password>table#profile tr#password</Password>
    <Phone>table#profile tr#phone</Phone>
    <Communication>table#profile tr#communication</Communication>
    <CommunicationText>table#profile tr#communication tbody tr:nth-child(1) td:nth-child(1)</CommunicationText>
    <CommunicationPrefs>input[type="radio"][name="comms_preference"]</CommunicationPrefs>
    <HomePhone>table#profile tr#phone tbody tr:nth-child(1)>td</HomePhone>
    <MobilePhone>table#profile tr#phone tbody tr:nth-child(2)>td</MobilePhone>
    <EditPhoneHome>input#edit-home-phone-za</EditPhoneHome>
    <EditPhoneMobile>input#edit-mobile-phone-za</EditPhoneMobile>
    <EditPhonePin>input#edit-mobile-pin</EditPhonePin>
	<EditPhoneErrorMessage>.messages.error ul li</EditPhoneErrorMessage>
    <EditPasswordCurrent>input#edit-current-password</EditPasswordCurrent>
    <EditPasswordNew>input#edit-password</EditPasswordNew>
    <EditPasswordConfirm>input#edit-password-confirm</EditPasswordConfirm>
    <EditPasswordErrorMessage>div#edit-password-wrapper div.form-error div[class='invalid']</EditPasswordErrorMessage>
    <EditPasswordPopupErrorMessage>form#wonga-my-account-profile-edit-password-form div.messages.error</EditPasswordPopupErrorMessage>
    <PopupSuccessTitle>h1#success</PopupSuccessTitle>
    <SubmitButton>input#edit-submit</SubmitButton>
    <PopupErrorMessage>#fancybox-content div.messages.error ul li:nth-child(1)</PopupErrorMessage>
  </MyPersonalDetailsPage>

  <ChangeMyAddressElement>
    <Title>#change-my-address</Title>
    <Flat>#edit-flat-za</Flat>
    <District>#edit-district-za</District>
    <HouseNumber>#edit-house-number-za</HouseNumber>
    <Town>#edit-town-za</Town>
    <County>#edit-county-za</County>
    <AddressPeriod>#edit-address-period-za</AddressPeriod>
    <Street>#edit-street-za</Street>
    <Postcode>#edit-postcode-za</Postcode>
    <PostcodeErrorForm>#edit-postcode-za-wrapper div.form-error div.invalid</PostcodeErrorForm>
  </ChangeMyAddressElement>
  
  <AboutUsPage>
    <OurCustomers>#block-wonga-6 div div ul li:nth-child(2) a</OurCustomers>
    <ResponsibleLending>#block-wonga-6 div div ul li:nth-child(3) a</ResponsibleLending>
    <News>#block-wonga-6 div div ul li:nth-child(1) a</News>
    <WhyUseUs>#block-wonga-6 div div ul li:nth-child(4) a</WhyUseUs>
    <WereDifferent>div.wongaTabs li:nth-child(1) a</WereDifferent>
    <WereDifferentTitle>div.panes div:nth-child(1) h2</WereDifferentTitle>
    <WereFast>div.wongaTabs li:nth-child(2) a</WereFast>
    <WereFastTitle>div.panes div:nth-child(2) h2</WereFastTitle>
    <WereResponsible>div.wongaTabs li:nth-child(3) a</WereResponsible>
    <WereResponsibleTitle>div.panes div:nth-child(3) h2</WereResponsibleTitle>
    <WongaMoments>div.wongaTabs li:nth-child(4) a</WongaMoments>
    <WongaMomentsTitle>div.panes div:nth-child(4) h2</WongaMomentsTitle>
  </AboutUsPage>

  <MySummaryPage>
	 <PromisedRepayAmount>#content-area p span</PromisedRepayAmount>
	 <PromisedRepayDate>#content-area p strong em</PromisedRepayDate>
    <ViewLoanDetailsButton>#cloud a.mod.details</ViewLoanDetailsButton>
    <!-- Loan details popup-->
   <PopupForm>#fancybox-content</PopupForm>
   <PopupMySummaryTitle>h1#my-summary</PopupMySummaryTitle>
   <PopupSummaryDetailsTable>table#summary-details</PopupSummaryDetailsTable>
  </MySummaryPage>

  <HomePage>
    <Title>div#block-wonga-1 h2:nth-child(1)</Title>
    <TitleText>Flexible, short-term loans that give you lots of control</TitleText>
  </HomePage>

  <TabsElement>
    <Advice>li:nth-child(4)>a</Advice>
    <MyAccount>li:nth-child(5)>a</MyAccount>
  </TabsElement>

  <ApplicationSection>
    <FormId>#wonga-apply-ln-form</FormId>
    <EditPin>#edit-mobile-pin</EditPin>
    <ChangeMobileButton>#add-mobile-ln</ChangeMobileButton>  
  </ApplicationSection>

  <ApplyPage>
    <EditMobileNumber>#edit-new-mobile-number-za</EditMobileNumber>
    <PopupSaveButton>#edit-submit[value='Save']</PopupSaveButton>
    <PopupCloseButton>#edit-submit[value='Close']</PopupCloseButton>
    <BankAccount>#edit-bank-account-masked</BankAccount>
  </ApplyPage>

  
