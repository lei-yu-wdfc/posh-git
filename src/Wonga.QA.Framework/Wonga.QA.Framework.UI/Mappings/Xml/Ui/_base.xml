<?xml version="1.0" encoding="utf-8" ?>
<Ui>
  <LoginPage>
    <Username>#edit-name</Username>
    <Password>#edit-pass</Password>
    <LoginButton>#edit-login</LoginButton>
    <ForgotPassword>.//*[@id='wonga-login-form']/div/div[5]/a</ForgotPassword>
  </LoginPage>

  <ForgotPasswordPage>
    <Username>#edit-email</Username>
    <Captcha>#edit-captcha-response</Captcha>
    <Next>#edit-next</Next>
  </ForgotPasswordPage>

  <MobilePinVerificationSection>
    <Fieldset>#pin-verification</Fieldset>
    <Pin>#edit-pin</Pin>
  </MobilePinVerificationSection>

  <DeclinedPage>
    <HeaderText>Application declined</HeaderText>
  </DeclinedPage>

  <AddressDetailsPage>
    <FormId>#wonga-apply-lzero-form</FormId>
    <District>#edit-district</District>
    <FlatNumber>#edit-flat</FlatNumber>
    <Town>#edit-town</Town>
    <County>#edit-county</County>
    <Street>#edit-street</Street>
    <Postcode>#edit-postcode</Postcode>
    <AddressPeriod>#edit-address-period</AddressPeriod>
    <NextButton>#edit-submit</NextButton>
    <PostOfficeBox>#edit-pobox-ca</PostOfficeBox>
  </AddressDetailsPage>

  <PersonalBankAccountDetailsPage>
    <FormId>#lzero-bank-form</FormId>
    <NextButton>#edit-submit</NextButton>
  </PersonalBankAccountDetailsPage>

	<DealDonePage>
		<HeaderText>Your application has been approved</HeaderText>
		<ContinueButtonLink>.application-success-button</ContinueButtonLink>
		<ContinueButton>#edit-submit</ContinueButton>
		<RepayDate>#content-area div div div:nth-child(1) div p:nth-child(2) strong em</RepayDate>
		<RepayAmount>#content-area div div div:nth-child(1) div p:nth-child(2) span</RepayAmount>
	</DealDonePage>

  <AcceptedPage>
    <FormId>#wonga-loan-approve-form</FormId>
    <TotalToRepay>div#content-area span.blue-text</TotalToRepay>
    <RepaymentDate>div#content-area em</RepaymentDate>
    <AcceptBusinessLoan>#terms-accept .button</AcceptBusinessLoan>
    <AcceptGuarantorLoan>#guarantor-accept .button</AcceptGuarantorLoan>
    <AgreementConfirm>#edit-agreement-confirm</AgreementConfirm>
    <DirectDebitConfirm>#edit-direct-debit-confirm</DirectDebitConfirm>
    <SubmitButton>#edit-accept</SubmitButton>
    <Initials1>#edit-initials</Initials1>
    <Initials2>#edit-initials2</Initials2>
    <Initials3>#edit-initials3</Initials3>
    <Signature>#edit-signature</Signature>
    <DateOfAgreement>#edit-date-of-agreement-ca-datepicker-popup-0</DateOfAgreement>
    <ContinueTermsButton>#edit-continue-terms</ContinueTermsButton>
    <ContinueDirectDebitButton>#edit-continue-debit</ContinueDirectDebitButton>
  </AcceptedPage>

  <BankAccountSection>
    <Fieldset>#bank-1</Fieldset>
    <BankName>#edit-bank-name</BankName>
    <BranchNumber>#edit-branch-number</BranchNumber>
    <BankAccountType>#edit-bank-account-type</BankAccountType>
    <SortCodePart1>#edit-sort-code-part1</SortCodePart1>
    <SortCodePart2>#edit-sort-code-part2</SortCodePart2>
    <SortCodePart3>#edit-sort-code-part3</SortCodePart3>
    <AccountNumber>#edit-account-number</AccountNumber>
    <BankPeriod>#edit-bank-period</BankPeriod>
    <InstitutionNumber>#edit-institution-number</InstitutionNumber>
  </BankAccountSection>

  <PersonalDebitCardPage>
    <FormId>#lzero-card-form</FormId>
    <NextButton>#edit-submit</NextButton>
  </PersonalDebitCardPage>

  <SlidersElement>
    <FormId>#wonga-sliders-form</FormId>
    <LoanAmount>#edit-loan-amount</LoanAmount>
    <LoanDuration>#edit-loan-duration</LoanDuration>
    <SubmitButton>#edit-submit</SubmitButton>
    <TotalAmount>span.loan-amount</TotalAmount>
    <TotalFees>span.loan-fees</TotalFees>
    <TotalToRepay>span.loan-total</TotalToRepay>
    <RepaymentDate>span.repayment-date</RepaymentDate>
    <AmountMinusButton>#wonga-amount-slider-wrapper button.slider-minus</AmountMinusButton>
    <AmountPlusButton>#wonga-amount-slider-wrapper button.slider-plus</AmountPlusButton>
    <DurationMinusButton>#wonga-duration-slider-wrapper button.slider-minus</DurationMinusButton>
    <DurationPlusButton>#wonga-duration-slider-wrapper button.slider-plus</DurationPlusButton>
  </SlidersElement>

  <LoginElement>
    <LoginTrigger>#login-trigger</LoginTrigger>
    <FormId>#wonga-login-form</FormId>
    <Email>#edit-name</Email>
    <Password>#edit-pass</Password>
    <LoginButton>#edit-login</LoginButton>
    <LogoutTrigger>#logout-trigger</LogoutTrigger>
  </LoginElement>

  <HelpElement>
    <HelpTrigger>help-trigger</HelpTrigger>
    <HelplistQuestions>#listQuestions</HelplistQuestions>
  </HelpElement>

  <FAQElement>
    <SelectQuestion>.//*[@id='faqQuestions']/option[@selected='selected']</SelectQuestion>
    <AnswerQuestion>.//*[@id='containerAnswer']/h2</AnswerQuestion>
  </FAQElement>

  <InternationalElement>
    <InternationalTrigger>.//*[@id='international-trigger']</InternationalTrigger>
    <InternationalPanelZa>.//*[@id='international-panel']/div/ul/li[1]/a</InternationalPanelZa>
    <InternationalPanelUk>.//*[@id='international-panel']/div/ul/li[2]/a</InternationalPanelUk>
  </InternationalElement>

  <PersonalDetailsPage>
    <FormId>#wonga-apply-lzero-form</FormId>
    <SlidersFormId>#wonga-alterable-loan-details-fieldset</SlidersFormId>
    <CheckPrivacyPolicy>#edit-privacy</CheckPrivacyPolicy>
    <CheckCanContact>input[type="radio"][name="update_option"]</CheckCanContact>
    <NextButton>#edit-submit</NextButton>
    <SliderToggler>#slider-toggler</SliderToggler>
    <TotalToRepay>span.loan-total</TotalToRepay>
    <RepaymentDate>span.repayment-date</RepaymentDate>
    <AmountMinusButton>#wonga-amount-slider-wrapper button.slider-minus</AmountMinusButton>
    <AmountPlusButton>#wonga-amount-slider-wrapper button.slider-plus</AmountPlusButton>
    <DurationMinusButton>#wonga-duration-slider-wrapper button.slider-minus</DurationMinusButton>
    <DurationPlusButton>#wonga-duration-slider-wrapper button.slider-plus</DurationPlusButton>
  </PersonalDetailsPage>

  <YourNameSection>
    <Fieldset>fieldset[id="name"]</Fieldset>
    <Title>#edit-title</Title>
    <FirstName>#edit-first-name</FirstName>
    <MiddleName>#edit-middle-name</MiddleName>
    <LastName>#edit-last-name</LastName>
  </YourNameSection>

  <YourDetailsSection>
    <IdNumber>#edit-id-number</IdNumber>
    <Fieldset>fieldset[id="details"]</Fieldset>
    <Dependants>#edit-dependants</Dependants>
    <Gender>input[type="radio"][name="gender"]</Gender>
    <DateOfBirthDay>#edit-date-of-birth-day</DateOfBirthDay>
    <DateOfBirthMonth>#edit-date-of-birth-month</DateOfBirthMonth>
    <DateOfBirthYear>#edit-date-of-birth-year</DateOfBirthYear>
    <HomeStatus>#edit-home-status</HomeStatus>
    <MaritalStatus>#edit-marital-status</MaritalStatus>
    <HomeLanguage>#edit-home-language</HomeLanguage>
  </YourDetailsSection>

  <ContactingYouSection>
    <Fieldset>fieldset[id="contact"]</Fieldset>
    <Email>#edit-email</Email>
    <EmailConfirm>#edit-email-confirm</EmailConfirm>
    <HomePhone>#edit-home-phone</HomePhone>
    <MobilePhone>#edit-mobile-phone</MobilePhone>
  </ContactingYouSection>

  <EmploymentDetailsSection>
    <Fieldset>fieldset[id="employment"]</Fieldset>
    <EmploymentStatus>#edit-employment-status</EmploymentStatus>
    <EmployerIndustry>#edit-employer-industry</EmployerIndustry>
    <EmployerName>#edit-employer-name</EmployerName>
    <EmploymentPosition>#edit-employment-position</EmploymentPosition>
    <TimeWithEmployerYears>#edit-employment-time-years</TimeWithEmployerYears>
    <TimeWithEmployerMonths>#edit-employment-time-months</TimeWithEmployerMonths>
    <MonthlyIncome>#edit-monthly-income</MonthlyIncome>
    <NextPaydayDate>#edit-next-payday-datepicker-popup-0</NextPaydayDate>
    <WorkPhone>#edit-work-phone</WorkPhone>
    <IncomeFrequency>#edit-income-frequency</IncomeFrequency>
    <SalaryPaidToBank>input[type="radio"][name="salary_to_bank"]</SalaryPaidToBank>
  </EmploymentDetailsSection>

  <AccountDetailsPage>
    <FormId>#lzero-account-setup-form</FormId>
    <NextButton>#edit-submit</NextButton>
  </AccountDetailsPage>

  <AccountDetailsSection>
    <Fieldset>#account-setup-1</Fieldset>
    <Password>#edit-password</Password>
    <PasswordConfirm>#edit-password-confirm</PasswordConfirm>
    <SecretQuestion>#edit-secret-question</SecretQuestion>
    <SecretAnswer>#edit-secret-answer</SecretAnswer>
  </AccountDetailsSection>

  <ProcessingPage>
    <!--<FormId>wonga-processing</FormId>-->
    <!--<ProcessingImage>#wonga-processing>img</ProcessingImage>-->
    <ProcessingText>We are processing your application</ProcessingText>
    <ProcessingTextContainer>.processing-text</ProcessingTextContainer>
  </ProcessingPage>

  <ProvinceSection>
    <Fieldset>fieldset#myprovince</Fieldset>
    <Province>#edit-province</Province>
    <PopupCloseLink>a#fancybox-close</PopupCloseLink>
  </ProvinceSection>

  <MySummaryPage>
    <Title>#my-summary</Title>
  </MySummaryPage>

  <MyPaymentsPage>
    <AddBankAccountButton>//tr[@class='accounts-add-bank']</AddBankAccountButton>
  </MyPaymentsPage>

  <SalesForceLoginPage>
    <Username>#username</Username>
    <Password>#password</Password>
    <LoginButton>#Login</LoginButton>
  </SalesForceLoginPage>
  
  <SalesForceHomePage>
    <SearchBox>#phSearchInput</SearchBox>
    <SearchButton>#phSearchButton</SearchButton>
  </SalesForceHomePage>

  <SalesForceSearchResultPage>
    <Customer>#Account_body table tbody tr:nth-child(2) th a</Customer>
  </SalesForceSearchResultPage>
</Ui>