<?xml version="1.0" encoding="utf-8" ?>
<Ui>
  <PersonalDetailsPage>
    <FormId>#lzero-personal-form</FormId>
  </PersonalDetailsPage>

  <EmploymentDetailsSection>    
    <NextPaydayDateDay>#edit-next-payday-day</NextPaydayDateDay>
    <NextPaydayDateMonth>#edit-next-payday-month</NextPaydayDateMonth>
    <NextPaydayDateYear>#edit-next-payday-year</NextPaydayDateYear>
  </EmploymentDetailsSection>

  <AddressDetailsPage>
    <FormId>#lzero-address-form</FormId>
    <LookupButton>#edit-submit-lookup</LookupButton>
    <AddressOptionsWrapper>#edit-address-options-wrapper</AddressOptionsWrapper>
    <PostcodeValid>#edit-postcode-lookup-uk-wrapper .form-error</PostcodeValid>
    <PostcodeLookup>#edit-postcode-lookup-uk</PostcodeLookup>
    <Postcode>#edit-postcode-lookup-uk</Postcode>
    <District>#edit-district</District>
    <HouseNumber>#edit-house-number</HouseNumber>
    <County>#edit-county</County>
    <Street>#edit-street</Street>
    <Town>#edit-town</Town>
    <PostcodeInForm>#edit-postcode-uk</PostcodeInForm>
    <AddressPeriod>#edit-address-period</AddressPeriod>
    <NextButton>#edit-submit</NextButton>
    <AddressOptions>#edit-address-options</AddressOptions>
    <HouseNumberErrorForm>#edit-house-number-wrapper div.invalid</HouseNumberErrorForm>
    <StreetErrorForm>#edit-street-wrapper div.invalid</StreetErrorForm>
    <TownErrorForm>#edit-town-wrapper div.invalid</TownErrorForm>
    <PostcodeErrorForm>#edit-postcode-uk-wrapper div.form-error div.invalid</PostcodeErrorForm>
    <AddressPeriodErrorForm>#edit-address-period-wrapper div.invalid</AddressPeriodErrorForm>
  </AddressDetailsPage>
  
  <BankAccountPage>
    <FormId>#lzero-business-bank-form</FormId>
    <NextButton>#edit-submit</NextButton>
  </BankAccountPage>

  <DealDonePage>
    <HeaderText>Your application has been accepted</HeaderText>
  </DealDonePage>

  <TopupDealDonePage>
    <AccountLink>#content-area h2 a</AccountLink>
  </TopupDealDonePage>

  <DebitCardSection>
    <Fieldset>#card-details</Fieldset>
    <CardType>#edit-card-type</CardType>
    <CardNumber>#edit-card-number</CardNumber>
    <CardName>#edit-card-name</CardName>
    <CardExpiryDateMonth>#edit-card-expiry-date-month</CardExpiryDateMonth>
    <CardExpiryDateYear>#edit-card-expiry-date-year</CardExpiryDateYear>
    <CardSecurityNumber>#edit-card-security</CardSecurityNumber>
    <CardStartDateMonth>#edit-card-start-date-month</CardStartDateMonth>
    <CardStartDateYear>#edit-card-start-date-year</CardStartDateYear>
  </DebitCardSection>

  <AcceptedPage>
    <FormId>#vanilla-agreement-form</FormId>
    <AgreementConfirm>#edit-accept</AgreementConfirm>
    <NodeWrap>.node-type-wonga-decision-page</NodeWrap>
  </AcceptedPage>

  <ApplicationSection>
    <FormId>#wonga-apply-ln-form</FormId>
    <SecurityCode>#edit-security-code</SecurityCode>
    <MinCash>#edit-min-cash</MinCash>
    <ChangeMobileButton>.mod</ChangeMobileButton>
  </ApplicationSection>

  <MyAccountNavigationSection>
    <MySummary>.menu-mlid-2869</MySummary>
    <MyPaymentsDetails>.menu-mlid-2870</MyPaymentsDetails>
    <MyPersonalDetails>.menu-mlid-2871</MyPersonalDetails>
  </MyAccountNavigationSection>
  
  <TopupSlidersElement>
    <FormId>#wonga-sliders-form</FormId>
    <LoanAmount>#edit-loan-amount</LoanAmount>
    <SubmitButton>#edit-submit</SubmitButton>
    <TotalAmount>span.loan-amount</TotalAmount>
    <TotalFees>span.loan-fees</TotalFees>
    <TotalToRepay>span.loan-total</TotalToRepay>
    <AmountMinusButton>#wonga-amount-slider-wrapper button.slider-minus</AmountMinusButton>
    <AmountPlusButton>#wonga-amount-slider-wrapper button.slider-plus</AmountPlusButton>
  </TopupSlidersElement>

  <SmallTopupSlidersElement>
    <FormId>#wonga-account-topup-form</FormId>
    <AmountSlider>#wonga-single-slider a</AmountSlider>
    <TopupLoanAmount>#edit-topup-amount-wrapper input#edit-topup-amount</TopupLoanAmount>
    <SubmitButton>#edit-submit</SubmitButton>
    <TopupAmount>span.credit-request-amount</TopupAmount>
    <TopupFees>span.credit-request-fees</TopupFees>
    <TopupToRepay>span.credit-request-subtotal</TopupToRepay>
    <TotalToRepay>span.credit-request-total</TotalToRepay>
    <RepaymentDate>span.loan-period-clarification</RepaymentDate>
    <AmountMinusButton>#wonga-single-slider-wrapper button.slider-minus</AmountMinusButton>
    <AmountPlusButton>#wonga-single-slider-wrapper button.slider-plus</AmountPlusButton>
  </SmallTopupSlidersElement>

  <ApplyPage>
    <EditMobileNumber>#edit-mobile-phone</EditMobileNumber>
    <PopupSaveButton>#edit-submit[value='Save']</PopupSaveButton>
    <PopupCloseButton>#edit-submit[value='Close']</PopupCloseButton>
  </ApplyPage>

  <PaymentCardsPage>
    <AddCardLink>a.bottom-link</AddCardLink>
  </PaymentCardsPage>

  <AccountingPage>
    <AddFileLink>li#liAddFile.ui-state-default a</AddFileLink>
    <CashOutLink>li#liTransactionsSearch.ui-state-default a</CashOutLink>
  </AccountingPage>

  <CashOutPage>
    <TransactionTypeOptions>select#TransactionType</TransactionTypeOptions>
    <FilterOptions>select#SearchBy</FilterOptions>
    <FilterValue>input#SearchByValue</FilterValue>
    <SearchButton>input#btnSearch</SearchButton>
    <SendPaymentCheckBox>input[name='PaymentSelected']</SendPaymentCheckBox>
    <CancelCheckBox>input[name='CancelSelected']</CancelCheckBox>
    <UpdateButton>#GridContainer > input[name='Command']</UpdateButton>
  </CashOutPage>

</Ui>