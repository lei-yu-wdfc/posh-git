<?xml version="1.0" encoding="utf-8" ?>
<UiMap>
  <YourNameSection>
    <Fieldset>fieldset#name</Fieldset>
  </YourNameSection>

  <YourDetailsSection>
    <Fieldset>fieldset#name</Fieldset>
    <IdNumber>#edit-id-number-ca</IdNumber>
  </YourDetailsSection>

  <ContactingYouSection>
    <HomePhone>#edit-home-phone-ca-part1</HomePhone>
    <HomePhoneP2>#edit-home-phone-ca-part2</HomePhoneP2>
    <HomePhoneP3>#edit-home-phone-ca-part3</HomePhoneP3>
    <MobilePhone>#edit-mobile-phone-ca-part1</MobilePhone>
    <MobilePhoneP2>#edit-mobile-phone-ca-part2</MobilePhoneP2>
    <MobilePhoneP3>#edit-mobile-phone-ca-part3</MobilePhoneP3>
  </ContactingYouSection>

  <EmploymentDetailsSection>
    <EmployerName>#edit-employer-name</EmployerName>
    <NextPaydayDate>#edit-next-payday-ca-datepicker-popup-0</NextPaydayDate>
    <SelfNextPaydayDate>#edit-next-income-due-ca-datepicker-popup-0</SelfNextPaydayDate>
  </EmploymentDetailsSection>


  <PersonalDetailsPage>
    <FormId>#lzero-personal-form</FormId>
    <CheckCanContact>#edit-contact-options</CheckCanContact>
    <AmountMinusButton>.wonga-amount-slider-wrapper .sliderminus</AmountMinusButton>
    <AmountPlusButton>.wonga-amount-slider-wrapper .sliderplus</AmountPlusButton>
    <DurationMinusButton>.wonga-duration-slider-wrapper .sliderminus</DurationMinusButton>
    <DurationPlusButton>.wonga-duration-slider-wrapper .sliderplus</DurationPlusButton>
  </PersonalDetailsPage>

  <DealDonePage>
    <HeaderText>Your application has been accepted</HeaderText>
  </DealDonePage>

  <AddressDetailsPage>
    <FormId>#lzero-address-form</FormId>
    <Postcode>#edit-postcode-ca</Postcode>
    <PostcodeErrorForm>div#edit-postcode-ca-wrapper div.invalid</PostcodeErrorForm>
    <HouseNumber>#edit-street-number-ca</HouseNumber>
    <Street>#edit-street-ca</Street>
    <PostOfficeBox>#edit-pobox-ca</PostOfficeBox>
    <Town>#edit-town-ca</Town>
    <AddressPeriod>#edit-address-period-ca</AddressPeriod>
    <PostcodeErrorForm>#edit-postcode-ca-wrapper div.form-error div.invalid</PostcodeErrorForm>
    <TownErrorForm>#edit-town-ca-wrapper div.invalid</TownErrorForm>
    <StreetErrorForm>#edit-street-ca-wrapper div.invalid</StreetErrorForm>
    <HouseNumberErrorForm>#edit-street-number-ca-wrapper div.invalid</HouseNumberErrorForm>
    <AddressPeriodErrorForm>#edit-address-period-ca-wrapper div.invalid</AddressPeriodErrorForm>
  </AddressDetailsPage>

  <PreviousAddresDetailsSection>
    <Fieldset>#previous-address</Fieldset>
    <PostCode>#edit-postcode-previous-ca</PostCode>
    <FlatNumber>#edit-street-number-previous-ca</FlatNumber>
    <Province>#edit-province-previous</Province>
    <Town>#edit-town-previous-ca</Town>
    <Street>#edit-street-previous-ca</Street>
  </PreviousAddresDetailsSection>

  <AccountDetailsSection>
    <Fieldset>fieldset#account-setup</Fieldset>
    <PasswordErrorForm>#edit-password-wrapper .form-error .invalid</PasswordErrorForm>
    <PasswordConfirmErrorForm>#edit-password-confirm-wrapper .form-error .invalid</PasswordConfirmErrorForm>
  </AccountDetailsSection>

  <PersonalBankAccountPage>
    <FormId>#lzero-bank-form</FormId>
  </PersonalBankAccountPage>


  <BankAccountSection>
    <Fieldset>fieldset#bank-1</Fieldset>
    <InstitutionNumber>#edit-institution-number</InstitutionNumber>
    <AccountNumber>#edit-account-number-ca</AccountNumber>
    <BankName>#edit-bank-name-ca</BankName>
  </BankAccountSection>

  <MobilePinVerificationSection>
    <Pin>#edit-pin-ca</Pin>
    <ResendPin>.resend-pin.mod</ResendPin>
    <ResendPinMessage>#fancybox-content div</ResendPinMessage>
    <ResendPinPopupClose>#fancybox-close</ResendPinPopupClose>
  </MobilePinVerificationSection>

  <AcceptedPage>
    <FormId>#wonga-loan-approve-form</FormId>
    <AgreementConfirm>#edit-loan-terms-confirm</AgreementConfirm>
    <DirectDebitConfirm>#edit-direct-debit-confirm</DirectDebitConfirm>
    <SubmitButton>#edit-accept</SubmitButton>
    <Initials1>#edit-initials-ca1</Initials1>
    <Initials2>#edit-initials-ca2</Initials2>
    <Initials3>#edit-initials-ca3</Initials3>
    <Signature>#edit-signature-ca</Signature>
    <DateOfAgreement>#edit-date-of-agreement-ca-datepicker-popup-0</DateOfAgreement>
    <ContinueTermsButton>#edit-continue-terms</ContinueTermsButton>
    <ContinueDirectDebitButton>#edit-continue-debit</ContinueDirectDebitButton>
    <LoanAmount>tr:first-child + tr>td:first-child + td</LoanAmount>
    <DetailsTable>div.loan-agreement-details-table tbody</DetailsTable>
    <PrincipalAmountBorrowed>tr:first-child + tr>td:first-child + td</PrincipalAmountBorrowed>
    <PrincipalAmountToBeTransfered>tr:first-child + tr + tr>td:first-child + td</PrincipalAmountToBeTransfered>
    <TotalCostOfCredit>tr:first-child+tr+tr+tr+tr>td:first-child + td</TotalCostOfCredit>
    <TotalAmountDueUnderTheAgreement>tr:first-child+tr+tr+tr+tr+tr+tr+tr>td:first-child + td</TotalAmountDueUnderTheAgreement>
    <PaymentDueDate>tr:first-child+tr+tr+tr+tr+tr+tr+tr+tr>td:first-child + td</PaymentDueDate>
  </AcceptedPage>

  <MyAccountNavigationSection>
    <!-- Should be extended-->
    <MySummary>div#block-wonga-6 ul.menu>li:first-child>a</MySummary>
    <MyPersonalDetails>div#block-wonga-6 ul.menu>li:first-child+li>a</MyPersonalDetails>
    <MyPaymentsDetails>div#block-wonga-6 ul.menu>li:first-child+li+li>a</MyPaymentsDetails>
  </MyAccountNavigationSection>

  <SurveyElement>
    <SurveyBox>div#wonga_survey_box</SurveyBox>
  </SurveyElement>

  <AboutUsPage>
    <OurCustomers>#block-wonga-6 div div ul li:first-child+li+li a</OurCustomers>
    <ResponsibleLending>#block-wonga-6 div div ul li:first-child+li a</ResponsibleLending>
    <News>#block-wonga-6 div div ul li:first-child a</News>
    <WhyUseUs>#block-wonga-6 div div ul li:first-child+li+li+li a</WhyUseUs>
    <WereDifferent>div.wongaTabs li:first-child a</WereDifferent>
    <WereDifferentTitle>div.panes div:first-child h2</WereDifferentTitle>
    <WereFast>div.wongaTabs li:first-child+li a</WereFast>
    <WereFastTitle>div.panes div:first-child+div h2</WereFastTitle>
    <WereResponsible>div.wongaTabs li:first-child+li+li a</WereResponsible>
    <WereResponsibleTitle>div.panes div:first-child+div+div h2</WereResponsibleTitle>
    <WongaMoments>div.wongaTabs li:first-child+li+li+li a</WongaMoments>
    <WongaMomentsTitle>div.panes div:first-child+div+div+div h2</WongaMomentsTitle>
  </AboutUsPage>

  <MySummaryPage>
    <PromisedRepayAmount>#content-area p span</PromisedRepayAmount>
    <PromisedRepayDate>#content-area p b em</PromisedRepayDate>
    <RepayButton>#cloud ul li a</RepayButton>
    <TotalToRepayAmountPopup>#fancybox-content div div b:first-child</TotalToRepayAmountPopup>
    <PromisedRepayDatePopup>#fancybox-content div div b:last-child</PromisedRepayDatePopup>
  </MySummaryPage>

  <HomePage>
    <Title>div#block-wonga-1 h2:first-child</Title>
    <TitleText>Flexible, short term loans that give you lots of control</TitleText>
    <ProvincePopupTitle>h1#select-province-title</ProvincePopupTitle>
    <YourProvince>select#selectprovince</YourProvince>
    <ThisIsMyProvince>a#province_right</ThisIsMyProvince>
  </HomePage>

  <TabsElement>
    <Advice>li:first-child+li+li+li>a</Advice>
    <MyAccount>li:first-child+li+li+li+li>a</MyAccount>
  </TabsElement>

  <ApplicationSection>
    <EditPin>#edit-mobile-pin-ca</EditPin>
    <FormId>#wonga-apply-ln-form</FormId>
    <ChangeMobileButton>#loan-application .form-markup-wrapper a</ChangeMobileButton>
  </ApplicationSection>

  <ApplyPage>
    <EditMobileNumber>#edit-new-mobile-number-ca</EditMobileNumber>
    <PopupSaveButton>#edit-submit[value='Save']</PopupSaveButton>
    <PopupCloseButton>#edit-submit[value='Close']</PopupCloseButton>
    <ResendPin>#repin div:first-child+div a</ResendPin>
    <ResendPinPopupClose>#fancybox-close</ResendPinPopupClose>
    <ResendPinPopupText>#fancybox-content div</ResendPinPopupText>
  </ApplyPage>

  <MyPersonalDetailsPage>
    <Postcode>table#profile tr#address tbody tr:first-child+tr+tr td</Postcode>
  </MyPersonalDetailsPage>

  <SlidersElement>
    <FormId>#wonga-sliders-form</FormId>
    <AmountSlider>.wonga-amount-slider-wrapper .sliderbit</AmountSlider>
    <DurationSlider>.wonga-duration-slider-wrapper .sliderbit</DurationSlider>
    <LoanAmount>#edit-loan-amount</LoanAmount>
    <LoanDuration>#edit-loan-duration</LoanDuration>
    <SubmitButton>#edit-submit</SubmitButton>
    <TotalAmount>span.loan-amount</TotalAmount>
    <TotalFees>span.loan-fees</TotalFees>
    <TotalToRepay>span.loan-total</TotalToRepay>
    <RepaymentDate>span.repayment-date</RepaymentDate>
    <AmountMinusButton>.wonga-amount-slider-wrapper .sliderminus</AmountMinusButton>
    <AmountPlusButton>.wonga-amount-slider-wrapper .sliderplus</AmountPlusButton>
    <DurationMinusButton>.wonga-duration-slider-wrapper .sliderminus</DurationMinusButton>
    <DurationPlusButton>.wonga-duration-slider-wrapper .sliderplus</DurationPlusButton>
  </SlidersElement>


</UiMap>
